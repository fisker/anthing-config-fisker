{
  "name": "anything-config-fisker",
  "version": "1.11.3",
  "description": "all configs for fisker",
  "repository": "fisker/anything-config-fisker",
  "bin": {
    "fisker-config": "cli.js"
  },
  "files": ["cli.js", "lib", "tools"],
  "scripts": {
    "dev": "rollup --config --watch",
    "clean": "rimraf lib",
    "prettier": "prettier **/*.{js,json,yml,md} --write",
    "lint": "eslint {src,tools}/**/*.js --fix",
    "build": "npm-run-all lint build:*",
    "build:js": "rollup --config",
    "prerelease": "npm-run-all clean build",
    "release": "npm-run-all release:*",
    "release:npm": "np --no-yarn --no-cleanup"
  },
  "dependencies": {
    "ansi-colors": "^3.2.4",
    "babel-plugin-transform-async-to-promises": "^0.8.9",
    "cp-file": "^6.2.0",
    "enquirer": "^2.3.0",
    "execa": "^1.0.0",
    "has-yarn": "^2.1.0",
    "latest-version": "^5.1.0",
    "read-pkg": "^5.0.0",
    "update-notifier": "^2.5.0",
    "write-pkg": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-fisker": "^4.3.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.9.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.2",
    "rollup-plugin-terser": "^4.0.4",
    "standard-version": "^5.0.2"
  },
  "bugs": {
    "url": "https://github.com/fisker/anthing-config-fisker/issues"
  },
  "homepage": "https://github.com/fisker/anthing-config-fisker#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
