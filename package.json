{
  "name": "anything-config-fisker",
  "version": "1.13.8",
  "description": "all configs for fisker",
  "repository": "fisker/anything-config-fisker",
  "bin": {
    "fisker-config": "cli.js"
  },
  "files": [
    "cli.js",
    "lib",
    "tools"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "rimraf lib",
    "dev": "rollup --config --watch",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build format dist"
  },
  "dependencies": {
    "ansi-colors": "^4.0.1",
    "babel-plugin-transform-async-to-promises": "^0.8.12",
    "cp-file": "^7.0.0",
    "enquirer": "^2.3.0",
    "execa": "^1.0.0",
    "has-yarn": "^2.1.0",
    "latest-version": "^5.1.0",
    "read-pkg": "^5.1.1",
    "sort-keys": "^3.0.0",
    "update-notifier": "^3.0.0",
    "write-pkg": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@fisker/commitlint-config": "^1.0.9",
    "@fisker/eslint-config": "^1.1.2",
    "@fisker/husky-config": "^1.1.0",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.17",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "markdownlint-cli": "^0.17.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-terser": "^5.0.0",
    "standard-version": "^6.0.1"
  },
  "bugs": {
    "url": "https://github.com/fisker/anthing-config-fisker/issues"
  },
  "homepage": "https://github.com/fisker/anthing-config-fisker#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false
}
