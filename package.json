{
  "name": "anything-config-fisker",
  "version": "2.0.8",
  "description": "all configs for fisker",
  "homepage": "https://github.com/fisker/anthing-config-fisker#readme",
  "bugs": {
    "url": "https://github.com/fisker/anthing-config-fisker/issues"
  },
  "repository": "fisker/anything-config-fisker",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "files": [
    "cli.js",
    "dist",
    "tools"
  ],
  "sideEffects": false,
  "bin": {
    "fisker-config": "cli.js"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dev": "rollup --config --watch",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build format dist"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "cp-file": "7.0.0",
    "enquirer": "2.3.2",
    "execa": "4.0.0",
    "has-yarn": "2.1.0",
    "is-path-in-cwd": "^3.0.0",
    "is-path-inside": "^3.0.2",
    "latest-version": "5.1.0",
    "read-pkg": "5.2.0",
    "sort-keys": "4.0.0",
    "update-notifier": "4.0.0",
    "write-pkg": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@commitlint/cli": "8.3.4",
    "@fisker/commitlint-config": "1.1.4",
    "@fisker/eslint-config": "1.6.0",
    "@fisker/husky-config": "1.1.5",
    "@fisker/lint-staged-config": "1.0.8",
    "@fisker/prettier-config": "1.0.24",
    "@rollup/plugin-commonjs": "11.0.1",
    "@rollup/plugin-json": "4.0.1",
    "@rollup/plugin-node-resolve": "7.0.0",
    "cz-conventional-changelog": "3.0.2",
    "del-cli": "3.0.0",
    "eslint": "6.8.0",
    "husky": "4.0.3",
    "lint-staged": "9.5.0",
    "markdownlint-cli": "0.21.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "rollup": "1.29.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-terser": "5.1.3",
    "sort-package-json": "1.38.1",
    "standard-version": "7.0.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
