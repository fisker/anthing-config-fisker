{
  "name": "anything-config-fisker",
  "version": "1.12.0",
  "description": "all configs for fisker",
  "repository": "fisker/anything-config-fisker",
  "bin": {
    "fisker-config": "cli.js"
  },
  "files": [
    "cli.js",
    "lib",
    "tools"
  ],
  "scripts": {
    "dev": "rollup --config --watch",
    "clean": "rimraf lib",
    "prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint **/*.{js,mjs,vue} --fix",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "release": "run-s clean lint prettier build dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn"
  },
  "dependencies": {
    "ansi-colors": "^3.2.4",
    "babel-plugin-transform-async-to-promises": "^0.8.9",
    "cp-file": "^7.0.0",
    "enquirer": "^2.3.0",
    "execa": "^1.0.0",
    "has-yarn": "^2.1.0",
    "latest-version": "^5.1.0",
    "read-pkg": "^5.0.0",
    "update-notifier": "^2.5.0",
    "write-pkg": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@commitlint/cli": "^7.5.2",
    "@fisker/commitlint-config": "^1.0.5",
    "@fisker/eslint-config": "^1.0.8",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.12",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "husky": "^2.0.0",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "standard-version": "^5.0.2"
  },
  "bugs": {
    "url": "https://github.com/fisker/anthing-config-fisker/issues"
  },
  "homepage": "https://github.com/fisker/anthing-config-fisker#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
